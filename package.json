{
	"name": "dynamodb-seeder",
	"version": "1.0.3",
	"description": "Seed your DynamoDB database",
	"type": "module",
	"main": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/FredPerr/dynamodb-seeder.git"
	},
	"files": [
		"dist"
	],
	"keywords": [
		"dynamodb",
		"cli",
		"seed"
	],
	"author": "Fred Perron",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/FredPerr/dynamodb-seeder/issues"
	},
	"homepage": "https://github.com/FredPerr/dynamodb-seeder#readme",
	"exports": {
		".": {
			"import": "./dist/index.js",
			"types": "./dist/index.d.ts"
		}
	},
	"bin": {
		"dyno": "./dist/cli.js"
	},
	"scripts": {
		"build": "rm -rf ./dist && tsc -p tsconfig.build.json && chmod +x ./dist/cli.js && cp ./package.json ./dist/package.json && cp ./src/demo.ts ./dist/demo.ts",
		"test": "vitest --no-watch run tests",
		"lint": "biome check",
		"lint:fix": "biome format --write",
		"local-db": "cd local-db && java -Djava.library.path=./DynamoDBLocal_lib -jar DynamoDBLocal.jar -sharedDb",
		"seed": "ts-node src/cli.ts"
	},
	"devDependencies": {
		"@biomejs/biome": "2.2.4",
		"@types/node": "^24.5.2",
		"typescript": "^5.9.2",
		"vitest": "^3.2.4"
	},
	"dependencies": {
		"@aws-sdk/client-dynamodb": "^3.893.0",
		"@aws-sdk/lib-dynamodb": "^3.893.0",
		"@aws-sdk/util-dynamodb": "^3.893.0",
		"@faker-js/faker": "^10.0.0",
		"commander": "^14.0.1",
		"ts-node": "^10.9.2"
	}
}
